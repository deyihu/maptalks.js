{
  "name": "maptalks",
  "version": "1.0.0-rc.20",
  "description": "A light JavaScript library to create integrated 2D/3D maps.",
  "license": "BSD-3-Clause",
  "author": "Fu Zhen",
  "keywords": [
    "javascript",
    "gis",
    "map",
    "2d-maps",
    "3d-maps",
    "geospatial"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maptalks/maptalks.js.git"
  },
  "bugs": {
    "url": "https://github.com/maptalks/maptalks.js/issues"
  },
  "homepage": "https://maptalks.org",
  "scripts": {
    "build": "npm run tsc && npm run build-styles && npm run build-dev && gulp minify",
    "build-dev": "npm run tsc &&rollup -c && gulp transform-proto",
    "build-test": "npm run tsc &&rollup -c --environment BUILD:test && gulp transform-proto",
    "build-styles": "gulp styles",
    "dev": "npm run tsc &&rollup --environment BUILD:dev -c -w",
    "doc": "npm run tsc && gulp doc",
    "lint": "eslint src/**/*.js test/**/*.js",
    "postversion": "gulp zip",
    "pretest": "npm run lint && npm run build-test",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "report-coverage": "codecov",
    "test": "karma start build/karma.cover.config.js",
    "testIE": "karma start build/karma.ie.config.js",
    "version": "npm run build && npm run doc && git add -A docs/api",
    "publish-next": "npm publish --tag next",
    "tsc": "npx tsc"
  },
  "unpkg": "dist/maptalks.js",
  "jsdelivr": "dist/maptalks.js",
  "module": "dist/maptalks.es-bundler.js",
  "module_browser": "dist/maptalks.es.js",
  "main": "dist/maptalks.js",
  "jsnext:main": "dist/maptalks.es-bundler.js",
  "style": "dist/maptalks.css",
  "dependencies": {
    "@maptalks/feature-filter": "^1.3.0",
    "@maptalks/function-type": "^1.3.1",
    "frustum-intersects": "^0.1.0",
    "rbush": "^2.0.2",
    "simplify-js": "^1.2.1",
    "zousan": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-maptalks": "^0.3.0",
    "eslint-plugin-mocha": "^5.0.0",
    "expect-maptalks": "^0.4.1",
    "expect.js": "^0.3.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "5.0.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-zip": "^4.0.0",
    "happen": "^0.3.1",
    "jsdoc": "^3.6.10",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-expect": "^1.1.3",
    "karma-expect-maptalks": "^0.1.5",
    "karma-happen": "^0.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "maptalks-jsdoc": "^0.2.5",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "rollup": "^3.23.0",
    "sinon": "^3.2.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "uglify-js": "^3.3.28",
    "zlib": "^1.0.5"
  }
}
